                                            <property name="label" translatable="yes">Keep Rows&#x00A0;&#x2060;&#x2335;</property>
                                            <property name="vexpand">true</property>
                                            <property name="valign">start</property>
                                            <property name="lines">2</property>
                                            <property name="wrap">true</property>
                                            <property name="justify">center</property>
                                            <property name="max-width-chars">0</property>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                    <layout>
                                      <property name="column">0</property>
                                      <property name="row">0</property>
                                      <property name="row-span">2</property>
                                    </layout>
                                    <style>
                                      <class name="flat"/>
                                    </style>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkMenuButton" id="RemoveRowsButton">
                                    <property name="name">remove-rows</property>
                                    <property name="vexpand">true</property>
                                    <property name="tooltip-text" translatable="yes">Remove Rows</property>
                                    <property name="menu-model">remove_rows_menu</property>
                                    <child>
                                      <object class="GtkBox">
                                        <property name="orientation">vertical</property>
                                        <property name="homogeneous">true</property>
                                        <child>
                                          <object class="GtkImage">
                                            <property name="icon-name">xxx-row-delete-symbolic</property>
                                          </object>
                                        </child>
                                        <child>
