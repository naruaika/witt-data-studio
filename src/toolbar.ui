<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="Adw" version="1.0"/>
  <requires lib="gtk" version="4.0"/>
  <template class="Toolbar" parent="GtkBox">
    <property name="orientation">vertical</property>

    <child>
      <object class="GtkScrolledWindow">
        <property name="vscrollbar-policy">never</property>
        <property name="hscrollbar-policy">external</property>
        <property name="hexpand">true</property>
        <child>
          <object class="GtkBox">
            <property name="orientation">horizontal</property>
            <property name="spacing">6</property>

            <!-- Command Palette -->
            <child>
              <object class="GtkButton">
                <property name="action-name">win.command-palette</property>
                <child>
                  <object class="GtkBox">
                    <property name="orientation">horizontal</property>
                    <property name="spacing">3</property>
                    <child>
                      <object class="GtkImage">
                        <property name="icon-name">xxx-search-symbolic</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel">
                        <property name="label" translatable="yes">Menus</property>
                      </object>
                    </child>
                  </object>
                </child>
                <style>
                  <class name="flat"/>
                  <class name="frame"/>
                </style>
              </object>
            </child>

            <!-- View Switcher -->
            <child>
              <object class="AdwInlineViewSwitcher" id="ViewSwitcher">
                <property name="halign">start</property>
                <property name="can-shrink">false</property>
                <property name="stack">ViewStack</property>
                <style>
                  <class name="flat"/>
                  <class name="round"/>
                </style>
              </object>
            </child>

          </object>
        </child>
        <style>
          <class name="mtoolbar-tab"/>
        </style>
      </object>
    </child>

    <!-- View Stack -->
    <child>
      <object class="AdwViewStack" id="ViewStack">

        <!-- Home Page -->
        <child>
          <object class="AdwViewStackPage" id="HomePage">
            <property name="name">home</property>
            <property name="title">Home</property>
            <property name="child">
              <object class="GtkScrolledWindow">
                <property name="vscrollbar-policy">never</property>
                <property name="hscrollbar-policy">external</property>
                <property name="hexpand">true</property>
                <child>
                  <object class="GtkBox">
                    <property name="orientation">horizontal</property>
                    <property name="spacing">6</property>

                    <!-- Clipboard Section -->
                    <child>
                      <object class="GtkBox" id="ClipboardSection">
                        <property name="name">clipboard</property>
                        <property name="orientation">horizontal</property>
                        <property name="spacing">6</property>
                        <child>
                          <object class="GtkBox">
                            <property name="orientation">vertical</property>
                            <property name="vexpand">false</property>
                            <property name="halign">start</property>
                            <property name="spacing">6</property>
                            <child>
                              <object class="GtkGrid">
                                <property name="vexpand">true</property>
                                <child>
                                  <object class="GtkButton" id="PasteButton">
                                    <property name="name">paste</property>
                                    <property name="vexpand">true</property>
                                    <property name="tooltip-text" translatable="yes">Paste</property>
                                    <property name="action-name">win.toolbar</property>
                                    <property name="action-target">'paste'</property>
                                    <child>
                                      <object class="GtkBox">
                                        <property name="orientation">vertical</property>
                                        <property name="homogeneous">true</property>
                                        <child>
                                          <object class="GtkImage">
                                            <property name="icon-name">xxx-paste-symbolic</property>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkLabel">
                                            <property name="label" translatable="yes">Paste</property>
                                            <property name="vexpand">true</property>
                                            <property name="valign">start</property>
                                            <property name="lines">2</property>
                                            <property name="wrap">true</property>
                                            <property name="justify">center</property>
                                            <property name="max-width-chars">0</property>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                    <layout>
                                      <property name="column">0</property>
                                      <property name="row">0</property>
                                      <property name="row-span">2</property>
                                    </layout>
                                    <style>
                                      <class name="flat"/>
                                    </style>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkButton" id="CutButton">
                                    <property name="name">cut</property>
                                    <property name="vexpand">true</property>
                                    <property name="tooltip-text" translatable="yes">Cut</property>
                                    <property name="action-name">win.toolbar</property>
                                    <property name="action-target">'cut'</property>
                                    <child>
                                      <object class="GtkBox">
                                        <property name="orientation">horizontal</property>
                                        <property name="spacing">6</property>
                                        <child>
                                          <object class="GtkImage">
                                            <property name="icon-name">xxx-cut-symbolic</property>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkLabel">
                                            <property name="label" translatable="yes">Cut</property>
                                            <property name="vexpand">true</property>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                    <layout>
                                      <property name="column">1</property>
                                      <property name="row">0</property>
                                    </layout>
                                    <style>
                                      <class name="flat"/>
                                    </style>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkButton" id="CopyButton">
                                    <property name="name">copy</property>
                                    <property name="vexpand">true</property>
                                    <property name="tooltip-text" translatable="yes">Copy</property>
                                    <property name="action-name">win.toolbar</property>
                                    <property name="action-target">'copy'</property>
                                    <child>
                                      <object class="GtkBox">
                                        <property name="orientation">horizontal</property>
                                        <property name="spacing">6</property>
                                        <child>
                                          <object class="GtkImage">
                                            <property name="icon-name">xxx-copy-symbolic</property>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkLabel">
                                            <property name="label" translatable="yes">Copy</property>
                                            <property name="vexpand">true</property>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                    <layout>
                                      <property name="column">1</property>
                                      <property name="row">1</property>
                                    </layout>
                                    <style>
                                      <class name="flat"/>
                                    </style>
                                  </object>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel">
                                <property name="label" translatable="yes">Clipboard</property>
                                <style>
                                  <class name="caption"/>
                                </style>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkSeparator">
                            <property name="orientation">vertical</property>
                          </object>
                        </child>
                      </object>
                    </child>

                    <!-- Input & Output Section -->
                    <child>
                      <object class="GtkBox" id="InputOutputSection">
                        <property name="name">input-output</property>
                        <property name="orientation">horizontal</property>
                        <property name="spacing">6</property>
                        <child>
                          <object class="GtkBox">
                            <property name="orientation">vertical</property>
                            <property name="vexpand">false</property>
                            <property name="halign">start</property>
                            <property name="spacing">6</property>
                            <child>
                              <object class="GtkGrid">
                                <property name="vexpand">true</property>
                                <child>
                                  <object class="GtkButton" id="ReadFileButton">
                                    <property name="name">read-file</property>
                                    <property name="vexpand">true</property>
                                    <property name="tooltip-text" translatable="yes">Read File</property>
                                    <property name="action-name">win.toolbar</property>
                                    <property name="action-target">'read-file'</property>
                                    <child>
                                      <object class="GtkBox">
                                        <property name="orientation">vertical</property>
                                        <property name="homogeneous">true</property>
                                        <child>
                                          <object class="GtkImage">
                                            <property name="icon-name">xxx-document-import-symbolic</property>
                                            <style>
                                              <class name="left-1px"/>
                                            </style>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkLabel">
                                            <property name="label" translatable="yes">Read File</property>
                                            <property name="vexpand">true</property>
                                            <property name="valign">start</property>
                                            <property name="lines">2</property>
                                            <property name="wrap">true</property>
                                            <property name="justify">center</property>
                                            <property name="max-width-chars">0</property>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                    <layout>
                                      <property name="column">0</property>
                                      <property name="row">0</property>
                                      <property name="row-span">2</property>
                                    </layout>
                                    <style>
                                      <class name="flat"/>
                                    </style>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkButton" id="OpenFileButton">
                                    <property name="name">open-file</property>
                                    <property name="vexpand">true</property>
                                    <property name="tooltip-text" translatable="yes">Open File</property>
                                    <property name="action-name">win.toolbar</property>
                                    <property name="action-target">'open-file'</property>
                                    <child>
                                      <object class="GtkBox">
                                        <property name="orientation">vertical</property>
                                        <property name="homogeneous">true</property>
                                        <child>
                                          <object class="GtkImage">
                                            <property name="icon-name">xxx-document-import-symbolic</property>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkLabel">
                                            <property name="label" translatable="yes">Open File</property>
                                            <property name="vexpand">true</property>
                                            <property name="valign">start</property>
                                            <property name="lines">2</property>
                                            <property name="wrap">true</property>
                                            <property name="justify">center</property>
                                            <property name="max-width-chars">0</property>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                    <layout>
                                      <property name="column">1</property>
                                      <property name="row">0</property>
                                      <property name="row-span">2</property>
                                    </layout>
                                    <style>
                                      <class name="flat"/>
                                    </style>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkButton" id="QueryDatabaseButton">
                                    <property name="name">query-database</property>
                                    <property name="vexpand">true</property>
                                    <property name="tooltip-text" translatable="yes">Query Database</property>
                                    <property name="action-name">win.toolbar</property>
                                    <property name="action-target">'query-database'</property>
                                    <child>
                                      <object class="GtkBox">
                                        <property name="orientation">vertical</property>
                                        <property name="homogeneous">true</property>
                                        <child>
                                          <object class="GtkImage">
                                            <property name="icon-name">xxx-database-symbolic</property>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkLabel">
                                            <property name="label" translatable="yes">Query Database</property>
                                            <property name="vexpand">true</property>
                                            <property name="valign">start</property>
                                            <property name="lines">2</property>
                                            <property name="wrap">true</property>
                                            <property name="justify">center</property>
                                            <property name="max-width-chars">0</property>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                    <layout>
                                      <property name="column">2</property>
                                      <property name="row">0</property>
                                      <property name="row-span">2</property>
                                    </layout>
                                    <style>
                                      <class name="flat"/>
                                    </style>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkButton" id="RecentSourcesButton">
                                    <property name="name">recent-sources</property>
                                    <property name="vexpand">true</property>
                                    <property name="tooltip-text" translatable="yes">Recent</property>
                                    <property name="action-name">win.toolbar</property>
                                    <property name="action-target">'recent-sources'</property>
                                    <child>
                                      <object class="GtkBox">
                                        <property name="orientation">horizontal</property>
                                        <property name="spacing">6</property>
                                        <child>
                                          <object class="GtkImage">
                                            <property name="icon-name">xxx-recently-viewed-symbolic</property>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkLabel">
                                            <property name="label" translatable="yes">Recent&#x00A0;&#x2060;&#x2335;</property>
                                            <property name="vexpand">true</property>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                    <layout>
                                      <property name="column">3</property>
                                      <property name="row">0</property>
                                    </layout>
                                    <style>
                                      <class name="flat"/>
                                    </style>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkButton" id="ExportDataButton">
                                    <property name="name">export-data</property>
                                    <property name="vexpand">true</property>
                                    <property name="tooltip-text" translatable="yes">Export</property>
                                    <property name="action-name">win.toolbar</property>
                                    <property name="action-target">'export-data'</property>
                                    <child>
                                      <object class="GtkBox">
                                        <property name="orientation">horizontal</property>
                                        <property name="spacing">6</property>
                                        <child>
                                          <object class="GtkImage">
                                            <property name="icon-name">xxx-document-export-symbolic</property>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkLabel">
                                            <property name="label" translatable="yes">Export</property>
                                            <property name="vexpand">true</property>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                    <layout>
                                      <property name="column">3</property>
                                      <property name="row">1</property>
                                    </layout>
                                    <style>
                                      <class name="flat"/>
                                    </style>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkButton" id="FileUtilitiesButton">
                                    <property name="name">file-utilities</property>
                                    <property name="vexpand">true</property>
                                    <property name="tooltip-text" translatable="yes">Utilities</property>
                                    <property name="action-name">win.toolbar</property>
                                    <property name="action-target">'file-utilities'</property>
                                    <child>
                                      <object class="GtkBox">
                                        <property name="orientation">horizontal</property>
                                        <property name="spacing">6</property>
                                        <child>
                                          <object class="GtkImage">
                                            <property name="icon-name">xxx-repo-artifact-symbolic</property>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkLabel">
                                            <property name="label" translatable="yes">Utilities&#x00A0;&#x2060;&#x2335;</property>
                                            <property name="vexpand">true</property>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                    <layout>
                                      <property name="column">3</property>
                                      <property name="row">1</property>
                                    </layout>
                                    <style>
                                      <class name="flat"/>
                                    </style>
                                  </object>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel">
                                <property name="label" translatable="yes">Input &amp; Output</property>
                                <style>
                                  <class name="caption"/>
                                </style>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkSeparator">
                            <property name="orientation">vertical</property>
                          </object>
                        </child>
                      </object>
                    </child>

                    <!-- Manage Columns Section -->
                    <child>
                      <object class="GtkBox" id="ManageColumnsSection">
                        <property name="name">manage-columns</property>
                        <property name="orientation">horizontal</property>
                        <property name="spacing">6</property>
                        <child>
                          <object class="GtkBox">
                            <property name="orientation">vertical</property>
                            <property name="vexpand">false</property>
                            <property name="halign">start</property>
                            <property name="spacing">6</property>
                            <child>
                              <object class="GtkGrid">
                                <property name="vexpand">true</property>
                                <child>
                                  <object class="GtkButton" id="ChooseColumnsButton">
                                    <property name="name">choose-columns</property>
                                    <property name="vexpand">true</property>
                                    <property name="tooltip-text" translatable="yes">Choose Columns</property>
                                    <property name="action-name">win.toolbar</property>
                                    <property name="action-target">'choose-columns'</property>
                                    <child>
                                      <object class="GtkBox">
                                        <property name="orientation">vertical</property>
                                        <property name="homogeneous">true</property>
                                        <child>
                                          <object class="GtkImage">
                                            <property name="icon-name">xxx-column-symbolic</property>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkLabel">
                                            <property name="label" translatable="yes">Choose Columns</property>
                                            <property name="vexpand">true</property>
                                            <property name="valign">start</property>
                                            <property name="lines">2</property>
                                            <property name="wrap">true</property>
                                            <property name="justify">center</property>
                                            <property name="max-width-chars">0</property>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                    <layout>
                                      <property name="column">0</property>
                                      <property name="row">0</property>
                                      <property name="row-span">2</property>
                                    </layout>
                                    <style>
                                      <class name="flat"/>
                                    </style>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkButton" id="RemoveColumnsButton">
                                    <property name="name">remove-columns</property>
                                    <property name="vexpand">true</property>
                                    <property name="tooltip-text" translatable="yes">Remove Columns</property>
                                    <property name="action-name">win.toolbar</property>
                                    <property name="action-target">'remove-columns'</property>
                                    <child>
                                      <object class="GtkBox">
                                        <property name="orientation">vertical</property>
                                        <property name="homogeneous">true</property>
                                        <child>
                                          <object class="GtkImage">
                                            <property name="icon-name">xxx-column-delete-symbolic</property>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkLabel">
                                            <property name="label" translatable="yes">Remove Columns</property>
                                            <property name="vexpand">true</property>
                                            <property name="valign">start</property>
                                            <property name="lines">2</property>
                                            <property name="wrap">true</property>
                                            <property name="justify">center</property>
                                            <property name="max-width-chars">0</property>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                    <layout>
                                      <property name="column">1</property>
                                      <property name="row">0</property>
                                      <property name="row-span">2</property>
                                    </layout>
                                    <style>
                                      <class name="flat"/>
                                    </style>
                                  </object>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel">
                                <property name="label" translatable="yes">Manage Columns</property>
                                <style>
                                  <class name="caption"/>
                                </style>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                        <object class="GtkSeparator">
                          <property name="orientation">vertical</property>
                        </object>
                      </child>
                      </object>
                    </child>

                    <!-- Reduce Rows Section -->
                    <child>
                      <object class="GtkBox" id="ReduceRowsSection">
                        <property name="name">reduce-rows</property>
                        <property name="orientation">horizontal</property>
                        <property name="spacing">6</property>
                        <child>
                          <object class="GtkBox">
                            <property name="orientation">vertical</property>
                            <property name="vexpand">false</property>
                            <property name="halign">start</property>
                            <property name="spacing">6</property>
                            <child>
                              <object class="GtkGrid">
                                <property name="vexpand">true</property>
                                <child>
                                  <object class="GtkMenuButton" id="KeepRowsButton">
                                    <property name="name">keep-rows</property>
                                    <property name="vexpand">true</property>
                                    <property name="tooltip-text" translatable="yes">Keep Rows</property>
                                    <property name="menu-model">keep_rows_menu</property>
                                    <child>
                                      <object class="GtkBox">
                                        <property name="orientation">vertical</property>
                                        <property name="homogeneous">true</property>
                                        <child>
                                          <object class="GtkImage">
                                            <property name="icon-name">xxx-row-symbolic</property>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkLabel">
                                            <property name="label" translatable="yes">Keep Rows&#x00A0;&#x2060;&#x2335;</property>
                                            <property name="vexpand">true</property>
                                            <property name="valign">start</property>
                                            <property name="lines">2</property>
                                            <property name="wrap">true</property>
                                            <property name="justify">center</property>
                                            <property name="max-width-chars">0</property>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                    <layout>
                                      <property name="column">0</property>
                                      <property name="row">0</property>
                                      <property name="row-span">2</property>
                                    </layout>
                                    <style>
                                      <class name="flat"/>
                                    </style>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkMenuButton" id="RemoveRowsButton">
                                    <property name="name">remove-rows</property>
                                    <property name="vexpand">true</property>
                                    <property name="tooltip-text" translatable="yes">Remove Rows</property>
                                    <property name="menu-model">remove_rows_menu</property>
                                    <child>
                                      <object class="GtkBox">
                                        <property name="orientation">vertical</property>
                                        <property name="homogeneous">true</property>
                                        <child>
                                          <object class="GtkImage">
                                            <property name="icon-name">xxx-row-delete-symbolic</property>
                                          </object>
                                        </child>
                                        <child>
                                          <object class="GtkLabel">
                                            <property name="label" translatable="yes">Remove Rows&#x00A0;&#x2060;&#x2335;</property>
                                            <property name="vexpand">true</property>
                                            <property name="valign">start</property>
                                            <property name="lines">2</property>
                                            <property name="wrap">true</property>
                                            <property name="justify">center</property>
                                            <property name="max-width-chars">0</property>
                                          </object>
                                        </child>
                                      </object>
                                    </child>
                                    <layout>
                                      <property name="column">1</property>
                                      <property name="row">0</property>
                                      <property name="row-span">2</property>
                                    </layout>
                                    <style>
                                      <class name="flat"/>
                                    </style>
                                  </object>
                                </child>
                              </object>
                            </child>
                            <child>
                              <object class="GtkLabel">
                                <property name="label" translatable="yes">Reduce Rows</property>
                                <style>
                                  <class name="caption"/>
                                </style>
                              </object>
                            </child>
                          </object>
                        </child>
                        <child>
                          <object class="GtkSeparator">
                            <property name="orientation">vertical</property>
                          </object>
                        </child>
                      </object>
                    </child>

                    <!-- Sort & Filter Section -->
                    <child>
                      <object class="GtkBox" id="SortAndFilterSection">
                        <property name="name">sort</property>
                        <property name="orientation">horizontal</property>
                        <property name="spacing">6</property>
                        <child>
                          <object class="GtkBox">
                            <property name="orientation">vertical</property>
                            <property name="vexpand">false</property>
                            <property name="halign">start</property>
                            <property name="spacing">6</property>
                            <child>
                              <object class="GtkGrid">
                                <property name="vexpand">true</property>
                                <child>
