<?xml version="1.0" encoding="UTF-8"?>
<interface>
  <requires lib="gtk" version="4.0"/>
  <template class="NodeEditor" parent="GtkOverlay">
    <property name="overflow">hidden</property>
    <child>
      <object class="GtkScrolledWindow" id="ScrolledWindow">
        <property name="hexpand">true</property>
        <property name="vexpand">true</property>
        <child>
          <object class="NodeCanvas" id="Canvas">
          <property name="height-request">16384</property>
          <property name="width-request">16384</property>
          <style>
            <class name="node-canvas"/>
          </style>
        </object>
        </child>
      </object>
    </child>
    <child type="overlay">
      <object class="NodeMinimap" id="Minimap">
        <property name="overflow">hidden</property>
        <property name="height-request">150</property>
        <property name="width-request">150</property>
        <property name="margin-top">20</property>
        <property name="margin-bottom">20</property>
        <property name="margin-start">20</property>
        <property name="margin-end">20</property>
        <property name="halign">end</property>
        <property name="valign">end</property>
        <style>
          <class name="view"/>
          <class name="frame"/>
        </style>
      </object>
    </child>
    <style>
      <class name="node-editor"/>
    </style>
  </template>
</interface>
